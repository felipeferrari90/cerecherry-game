
{
    "version": 2,
    
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["*"] }
        },
        {
            "src": "public/**",
            "use": "@vercel/static",
            "config": { "includeFiles": ["public/**"] }
        }
    ],
    "routes": [
        {
            "src": "/" ,
            "dest": "public/index.html"
        }
    ]
}
