
{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static",
            "config": { "includeFiles": ["public/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/fase",
            "dest": "server.js"
          },
          {
            "src": "/(.+)",
            "dest": "public/$1"
          }
    ]
}
